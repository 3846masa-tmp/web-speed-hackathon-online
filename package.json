{
  "name": "web-speed-hackathon-online",
  "version": "0.0.1",
  "license": "Proprietary",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm-run-all clean -p build:css build:backend build:webpack -s build:compress",
    "build:backend": "babel --config-file ./.backend.babelrc --out-dir dist/backend src/backend",
    "build:css": "postcss src/client/app.css -d dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build:compress": "gzipper c --brotli --include html,css,js ./dist",
    "fmt": "prettier --write 'src/**/*.{js,css}'",
    "serve": "nodemon dist/backend/server.js"
  },
  "devDependencies": {
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "file-loader": "^6.0.0",
    "gzipper": "^4.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.0.8",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.27",
    "postcss-cli": "^7.1.0",
    "postcss-custom-properties": "^9.1.1",
    "postcss-import": "^12.0.1",
    "prettier": "^2.0.1",
    "rimraf": "^3.0.2",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^5.0.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,css}": "prettier --write"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-transform-modules-commonjs": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.1",
    "agentkeepalive": "^4.1.3",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "dayjs": "^1.8.29",
    "dnscache": "^1.0.2",
    "express": "^4.17.1",
    "express-static-gzip": "^2.0.7",
    "lodash.chunk": "^4.2.0",
    "lodash.shuffle": "^4.2.0",
    "normalize.css": "^8.0.1",
    "preact": "^10.4.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redaxios": "^0.2.0",
    "redux": "^4.0.5",
    "regenerator-runtime": "^0.13.5"
  }
}
